{
	"name": "olvlvl/doctrine-generators",
	"type": "library",
	"description": "Generators for Doctrine's hydrators and proxies.",
	"keywords": [
		"doctrine",
		"generator",
		"hydrator",
		"proxy"
	],
	"license": "BSD-3-Clause",
	"authors": [
		{
			"name": "Olivier Laviale",
			"email": "olivier.laviale@gmail.com",
			"homepage": "https://olvlvl.com/",
			"role": "Developer"
		}
	],
	"support": {
		"issues": "https://github.com/olvlvl/doctrine-generators/issues",
		"source": "https://github.com/olvlvl/doctrine-generators"
	},
	"prefer-stable": true,
	"require": {
		"php": ">=7.0"
	},
	"autoload": {
		"psr-4": {
			"olvlvl\\DoctrineGenerators\\": "lib/"
		}
	},
	"autoload-dev": {
		"psr-4": {
			"olvlvl\\DoctrineGenerators\\": "tests/"
		}
	},
	"require-dev": {
		"alcaeus/mongo-php-adapter": "^1.1",
		"doctrine/mongodb-odm": "^1.2",
		"symfony/yaml": "^3.4"
	},
	"config": {
		"platform": {
			"ext-mongo": "1.6.16"
		}
	}
}
